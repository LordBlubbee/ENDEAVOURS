    - ReplaceDialog: {fileID: 11400000, guid: f00a08ba3a5f1aa4bb918d6c3f3360c6, type: 2}
      Prerequisites:
      - {fileID: 11400000, guid: 82e95b99088449b488dc0499ee67d3f3, type: 2}
    - ReplaceDialog: {fileID: 11400000, guid: a654a724f75c70640983b6a5c590f18b, type: 2}
      Prerequisites:
      - {fileID: 11400000, guid: b32172dce6df9864b9d23047a4235f27, type: 2}
    TriggerEvent: {fileID: 0}
  - ChoiceText:
      BaseText: We have no news for you, but we are very much open to trade. We are
        requesting to dock.
      Speaker: {fileID: 0}
      Alternatives: []
    Prerequisites: []
    DialogResult: {fileID: 11400000, guid: 1a151b1c81b16144888f05cd0e773fd6, type: 2}
    AlternativeResults: []
    TriggerEvent: {fileID: 0}
  - ChoiceText:
      BaseText: Surrender your cargo to us immediately or be destroyed, Nomaden vessel.
        We will enforce this by force is necessary.
      Speaker: {fileID: 0}
