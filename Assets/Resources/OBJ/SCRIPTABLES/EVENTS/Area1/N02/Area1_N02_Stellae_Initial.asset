    TriggerEvent: {fileID: 0}
  - ChoiceText:
      BaseText: We will sadly not be able to assist you, Stellae. Perhaps we can
        exchange some goods and stories instead? We are in need of general information.
      VariantTexts: []
      Speaker: {fileID: 0}
      Alternatives: []
    Prerequisites: []
