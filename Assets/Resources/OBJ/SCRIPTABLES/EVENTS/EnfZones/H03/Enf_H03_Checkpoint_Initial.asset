    AlternativeResults: []
    TriggerEvent: {fileID: 0}
  - ChoiceText:
      BaseText: <color=yellow>[Reputation] We are allies of the enforcement operations
        in this area! If you cross-reference our ID, you are sure to find us. Please
        let us through.
      VariantTexts: []
